{
	"extends": ["eslint-config-synacor", "plugin:import/errors", "plugin:import/warnings"],
	"plugins": ["import"],
	"rules": {
		"max-len": [
			"error",
			{
				"code": 100,
				"tabWidth": 2,
				"ignoreUrls": true,
				"ignoreComments": false,
				"ignoreRegExpLiterals": true,
				"ignoreStrings": true,
				"ignoreTemplateLiterals": true
			}
		]
	}
}
